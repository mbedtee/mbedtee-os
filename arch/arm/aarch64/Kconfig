if AARCH64

config CFLAGS
	string "AARCH64_CFLAGS"
	default "-march=armv8-a"
	help
		Define the default compile FLAGS.

config PIC_CFLAGS
	string "AARCH64_PIC_CFLAGS"
	default "-fPIC"
	help
		Define the default compile PIC FLAGS.

config VA_BITS
	int "VA Bits"
	default 39

endif
